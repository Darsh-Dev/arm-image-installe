echo "= Writing u-boot-sunxi-with-spl.bin ...."
#sudo dd if=/tmp/root/usr/share/uboot/$TARGET/u-boot-sunxi-with-spl.bin of=$MEDIA bs=1024 seek=8; sync; sleep 5
sudo dd if=/tmp/$TARGET/u-boot-sunxi-with-spl.bin of=$MEDIA bs=1024 seek=8; sync; sleep 5

